    <webLinks>
        <fullName>Apply_for_User_License</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Assign User License</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}


if(&apos;{!QIPP_Contacts__c.User_License__c}&apos; != &apos;&apos;)
{
   alert(&apos;User License is Present&apos;)
}

else
{

var result=sforce.apex.execute(&quot;QIPPContactUserLicense&quot;,&quot;ApplyForUserLicense&quot;,{contactid:&quot;{!QIPP_Contacts__c.Id}&quot;});

if(result == &quot;ok&quot;)
{
   alert(&apos;Your request has been processed successfully. An email notification will be sent to the support team for creating the user account. {!QIPP_Contacts__c.Name} will be receiving an email with login credentials from Salesforce.com&apos;);
}
else if(result == &quot;Business Function is not present&quot;)
{
   alert(result);
}
else if(result == &quot;NSN ID is not present&quot;)
{
   alert(result);
}
else
{
   alert(&apos;We encountered an issue while processing your request&apos;);
}

}</url>
    </webLinks>
    <webLinks>
        <fullName>Remove_User_License</fullName>
        <availability>online</availability>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Remove User License</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}

if(&apos;{!QIPP_Contacts__c.User_License__c}&apos; == &apos;&apos;)
{
   alert(&apos;User License is not Present&apos;)
}

else if(confirm(&apos;Are you sure you want to remove license for this user?&apos;)==true)
{
var result=sforce.apex.execute(&quot;QIPPContactUserLicense&quot;,&quot;RemoveUserLicense&quot;,{contactid:&quot;{!QIPP_Contacts__c.Id}&quot;});

if(result == &quot;ok&quot;)
   alert(&apos;You request for Remove User License has been successfully proceed. &apos;);
}

else
{
}</url>
    </webLinks>
