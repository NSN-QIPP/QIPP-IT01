    <validationRules>
        <fullName>Check_User_Email</fullName>
        <active>true</active>
        <description>Users should ONLY be able to create / update their own record, except users in the Audit Manager role.</description>
        <errorConditionFormula>NOT (

OR ( $UserRole.Name = &apos;QIPP Audit Manager&apos;,  $Profile.Name = &apos;System Administrator&apos;,
Audit_Name__r.Email__c =  $User.Email )

)</errorConditionFormula>
        <errorMessage>Users are ONLY allowed to create / update their own record, unless you are a QIPP Administrator (please contact your QIPP Administrator for further assistance).</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Check_for_Process_Audits</fullName>
        <active>true</active>
        <errorConditionFormula>AND (
(ISPICKVAL( Process_Audits__c , &apos;N/A&apos;)),

 OR (Care_Process_8_4__c = true,
 Create_Process_8_3__c = true,
 Delivery_Process_8_2__c =true, 
 Sales_Process_8_1__c = true) 
)</errorConditionFormula>
        <errorDisplayField>Process_Audits__c</errorDisplayField>
        <errorMessage>Process Audits should not be N/A when one of the following is selected - Sales Process 8.1, Delivery Process 8.2, Create Process 8.3 or Care Process 8.4</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Disclaimer_Confirmation</fullName>
        <active>true</active>
        <description>Obtain user&apos;s approval in order to save the record</description>
        <errorConditionFormula>I_agree_to_proceed__c = false</errorConditionFormula>
        <errorDisplayField>I_agree_to_proceed__c</errorDisplayField>
        <errorMessage>Provide your approval in order to save this information.</errorMessage>
    </validationRules>
