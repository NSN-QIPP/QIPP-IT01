    <validationRules>
        <fullName>Buffer_Days_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISBLANK(PI_Data__c),OR(ISNULL(Buffer_Days__c),Buffer_Days__c &lt;=0))</errorConditionFormula>
        <errorDisplayField>ActualCalculation__c</errorDisplayField>
        <errorMessage>Buffer Days should be &gt;= 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE2_KPI_Equals_PI_Rule</fullName>
        <active>true</active>
        <description>This rule is triggered to check the KPI = PI functionality</description>
        <errorConditionFormula>AND(NOT(ISBLANK(PI_Data__c)),NOT(ISBLANK(Buffer_Days__c)))</errorConditionFormula>
        <errorMessage>Buffer Days should be blank for PI=KPI</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_KPI_Equals_PI_Rule</fullName>
        <active>true</active>
        <description>This rule is triggered to check the KPI = PI functionality</description>
        <errorConditionFormula>AND(!ISBLANK(PI_Data__c),!ISBLANK(Buffer_Days__c),OR(!ISBLANK(ScheduledDate__c),!ISBLANK(TEXT(FrequencyinMonth__c))),TEXT(GeoScope__c) != TEXT(PI_Data__r.GeoScope__c),TEXT(CategoryScope__c) != TEXT(PI_Data__r.CategoryScope__c),TEXT(BUScope__c) != TEXT(PI_Data__r.BUScope__c))</errorConditionFormula>
        <errorMessage>For KPI = PI, Scheduled Date,Buffer Days and Frequency Month should be blank AND Geo scope, Category Scope, Product Scope should match with that of the selected PI Definition</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_KPI_NotEquals_PI_Rule</fullName>
        <active>true</active>
        <description>This rule is triggered to force the user to provide a scheduled date and a frequency value provided the PI data is blank</description>
        <errorConditionFormula>AND(ISBLANK(PI_Data__c),ISBLANK( TEXT(FrequencyinMonth__c)))</errorConditionFormula>
        <errorMessage>Please provide the frequency in months</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_KPI_Published_Stage</fullName>
        <active>true</active>
        <description>KPI stage should be Published if PI Data not NULL or KPI Calculation Canvas filled.</description>
        <errorConditionFormula>If( Formula_Count__c = 0,true,false) &amp;&amp; ISBLANK(PI_Data__c)&amp;&amp;  ISPICKVAL( LifecycleStage__c , &quot;Published&quot;)</errorConditionFormula>
        <errorDisplayField>ActualCalculation__c</errorDisplayField>
        <errorMessage>Before Published you must have to complete KPI Calculation</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_SPE_User_Profile_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>$Profile.Name== &quot;SPE User&quot; &amp;&amp;  ISCHANGED(KPI_Title__c)</errorConditionFormula>
        <errorMessage>You are not allowed to edit the record</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_Schedule_date_validation</fullName>
        <active>false</active>
        <description>This validation restricts user from entering past date as scheduled date</description>
        <errorConditionFormula>AND (ISNEW(), ISBLANK(PI_Data__c), NOT(ISBLANK(ScheduledDate__c)),ScheduledDate__c &lt;=TODAY())</errorConditionFormula>
        <errorDisplayField>ScheduledDate__c</errorDisplayField>
        <errorMessage>Scheduled Date can only be a future date and cannot be left blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_special_character_not_allowed</fullName>
        <active>true</active>
        <description>Name should be start with Alphanumeric only</description>
        <errorConditionFormula>NOT(REGEX(KPI_Title__c, &quot;[a-zA-Z 0-9]{1}.*&quot;))</errorConditionFormula>
        <errorDisplayField>ActualCalculation__c</errorDisplayField>
        <errorMessage>Name should be start with Alphanumeric only</errorMessage>
    </validationRules>
