        <fullName>New_Action_Plan</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>New Action Plan</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)} 
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)} 

var classification = &apos;{!Finding__c.Classification__c}&apos;; 
var findingId = &apos;{!Finding__c.Id}&apos;; 
var findingname = &apos;{!Finding__c.Name}&apos;; 

if(classification == &apos;Opportunity For Improvement&apos; || classification == &apos;Strength&apos;){ 
var urlAP= &apos;/a15/e?RecordType=012U00000001eag&amp;CF00NU0000003NgtS={!Finding__c.Name}&amp;CF00NU0000003NgtS_lkid={!Finding__c.Id}&amp;retURL=%2F{!Finding__c.Id}&apos;; 
} 
else if(classification == &apos;Non-Conformance Major&apos; || classification == &apos;Non-Conformance Minor&apos;){ 
var urlAP= &apos;/a15/e?RecordType=012U00000001eaf&amp;CF00NU0000003NgtS={!Finding__c.Name}&amp;CF00NU0000003NgtS_lkid={!Finding__c.Id}&amp;retURL=%2F{!Finding__c.Id}&apos;; 
} 
navigateToUrl(urlAP);</url>
