    <validationRules>
        <fullName>Validate_Action_Plan_Start_Date</fullName>
        <active>true</active>
        <description>Actual Date should always be GREATER than Action Plan Start Date.</description>
        <errorConditionFormula>Action_Plan_Start_Date__c &gt;  Action_Plan_Actual_Completion_Date__c</errorConditionFormula>
        <errorDisplayField>Action_Plan_Actual_Completion_Date__c</errorDisplayField>
        <errorMessage>Action Plan Closing Date should always be GREATER than Action Plan Start Date</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Action_Plan_State</fullName>
        <active>true</active>
        <description>Cannot change Action Plan State  to completed unless Corrective action and Preventive actions are also complete.</description>
        <errorConditionFormula>AND(
 ISPICKVAL(   Action_Plan_State__c , &quot;Complete&quot;),

  OR( NOT(ISPICKVAL(Action_Plan_State_CA__c, &quot;Complete&quot;)),
      NOT(ISPICKVAL(Action_Plan_State_PA__c, &quot;Complete&quot;)) )

)</errorConditionFormula>
        <errorDisplayField>Action_Plan_State__c</errorDisplayField>
        <errorMessage>Cannot change Action Plan State  to &quot;Complete&quot; unless Corrective action and Preventive actions are also complete.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Analyzed_Date</fullName>
        <active>true</active>
        <description>Cannot change Action Plan (RCA) State  to Analyzed unless an Analyzed Date is entered</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Action_Plan_State_RCA__c , &quot;Analyzed&quot;),

ISBLANK(Analyzed_Date__c) 

)</errorConditionFormula>
        <errorDisplayField>Action_Plan_State_RCA__c</errorDisplayField>
        <errorMessage>Action Plan State (RCA) cannot be set to &quot;Analyzed&quot; unless Analyzed Date is populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Corrective_Action_Plan_State</fullName>
        <active>false</active>
        <description>Cannot change Correction Action Plan State  to completed unless an ACTUAL date is entered for Corrective Action.</description>
        <errorConditionFormula>AND(
 ISPICKVAL( Action_Plan_State_CA__c , &quot;Complete&quot;),

ISBLANK(Actual_Completion_Date_CA__c) 

)</errorConditionFormula>
        <errorDisplayField>Action_Plan_State_CA__c</errorDisplayField>
        <errorMessage>Action Plan State cannot be set to &quot;Complete&quot; unless Actual completion date is populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Preventive_Action_Plan_State</fullName>
        <active>false</active>
        <description>Cannot change Preventive Action Plan State  to completed unless an ACTUAL date is entered for Corrective Action.</description>
        <errorConditionFormula>AND(
 ISPICKVAL(  Action_Plan_State_PA__c  , &quot;Complete&quot;),

ISBLANK(Actual_Completion_Date_PA__c) 

)</errorConditionFormula>
        <errorDisplayField>Action_Plan_State_PA__c</errorDisplayField>
        <errorMessage>Action Plan State cannot be set to &quot;Complete&quot; unless Actual completion date is populated.</errorMessage>
    </validationRules>
