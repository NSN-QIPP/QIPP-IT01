    <validationRules>
        <fullName>SPE_Frequency</fullName>
        <active>true</active>
        <description>This rule puts a check on the Frequency for Survey PIs</description>
        <errorConditionFormula>AND(ISPICKVAL(DataAcquisitionMethod__c, &apos;Survey&apos;), NOT(ISPICKVAL(Frequency__c,&apos;0&apos;)))</errorConditionFormula>
        <errorDisplayField>Frequency__c</errorDisplayField>
        <errorMessage>For Survey PIs, Frequency can not be set to any values other than 0</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_PIDueDateRequired</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(DataAcquisitionMethod__c,&apos;Data Load&apos;)&amp;&amp; ISBLANK( PIUploadDuedate__c )</errorConditionFormula>
        <errorDisplayField>PIUploadDuedate__c</errorDisplayField>
        <errorMessage>Enter PI Upload Due date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_PIUploadDueDate_Blank</fullName>
        <active>true</active>
        <description>For survey based PI, the upload due date should be blank</description>
        <errorConditionFormula>ISPICKVAL(DataAcquisitionMethod__c,&apos;Survey&apos;)&amp;&amp; NOT(ISBLANK( PIUploadDuedate__c ))</errorConditionFormula>
        <errorDisplayField>PIUploadDuedate__c</errorDisplayField>
        <errorMessage>For Survey based PI, PI Upload due date should be blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_PIdatatypeNumberifmultiselectchoice</fullName>
        <active>true</active>
        <description>If the attached question answer is multiselect choice option then PI data type must be Number.</description>
        <errorConditionFormula>ISPICKVAL(SurveyQuestion__r.AnswerType__c,&apos;Picklist&apos;) &amp;&amp; NOT(ISPICKVAL(ValuesType__c,&apos;Number&apos;))</errorConditionFormula>
        <errorDisplayField>ValuesType__c</errorDisplayField>
        <errorMessage>Question answer is multiselect choice option then PI data type must be Number.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_QuestionIDRequiredForSurveyBased</fullName>
        <active>true</active>
        <errorConditionFormula>ISBLANK(SurveyQuestion__c) &amp;&amp; (ISPICKVAL(DataAcquisitionMethod__c,&apos;Survey&apos;) || ISPICKVAL(DataAcquisitionMethod__c,&apos;Standalone Survey&apos;))</errorConditionFormula>
        <errorDisplayField>SurveyQuestion__c</errorDisplayField>
        <errorMessage>For Survey / Standalone Survey Based Data Acquisition Method, Question ID can not be kept blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_Question_ID</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(DataAcquisitionMethod__c,&apos;Data Load&apos;)&amp;&amp; NOT(ISBLANK( SurveyQuestion__c))</errorConditionFormula>
        <errorDisplayField>SurveyQuestion__c</errorDisplayField>
        <errorMessage>If Data Acquisition Method is Data load then Question ID must be blank.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_StageValidation</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISPICKVAL(PRIORVALUE(LifecycleStage__c),&quot;Obsolete&quot;),OR( ISPICKVAL(LifecycleStage__c,&quot;Published&quot;),ISPICKVAL(LifecycleStage__c,&quot;Draft&quot;)))</errorConditionFormula>
        <errorMessage>The Life-cycle stage must be Draft before converted to published</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_StatusRestriction</fullName>
        <active>false</active>
        <errorConditionFormula>AND(ISNEW(), OR(ISPICKVAL(LifecycleStage__c,&quot;Published&quot;), 
ISPICKVAL(LifecycleStage__c,&quot;Obsolete&quot;) ))</errorConditionFormula>
        <errorDisplayField>LifecycleStage__c</errorDisplayField>
        <errorMessage>Lifecycle Stage must be Draft</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_Upload_Due_Date_cannot_exceed_25th</fullName>
        <active>false</active>
        <description>Upload due date field should not exceed 25th of a month</description>
        <errorConditionFormula>DAY(PIUploadDuedate__c)&gt;25</errorConditionFormula>
        <errorDisplayField>PIUploadDuedate__c</errorDisplayField>
        <errorMessage>PI Upload Due date cannot exceed 25th.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>SPE_special_character_not_allowed</fullName>
        <active>true</active>
        <description>Name should be start with Alphanumeric only</description>
        <errorConditionFormula>NOT(REGEX(PI_Title__c, &quot;[a-zA-Z 0-9]{1}.*&quot;))</errorConditionFormula>
        <errorDisplayField>CategoryScope__c</errorDisplayField>
        <errorMessage>Name should be start with Alphanumeric only</errorMessage>
    </validationRules>
