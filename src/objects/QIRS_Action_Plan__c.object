        <formula>IF( (ISPICKVAL( Status__c , &apos;Closed&apos;) || (ISPICKVAL( Status__c , &apos;Canceled&apos;))) , 

TEXT( FLOOR( (Status_Date1__c   -  CreatedDate)) )+ &apos;Days&apos;+
&apos;/&apos;+TEXT(ROUND(  MOD((Status_Date1__c   -  CreatedDate)*24,24)   ,0))+&apos;hours&apos;,

TEXT( FLOOR( (NOW()   -  CreatedDate)) )+ &apos;Days&apos;+
&apos;/&apos;+TEXT(ROUND(  MOD((NOW()   -  CreatedDate)*24,24)   ,0))+&apos;hours&apos;)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
