    <webLinks>
        <fullName>Cancel</fullName>
        <availability>online</availability>
        <description>Cancel button will cancel the action plan.</description>
        <displayType>button</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Cancel</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/connection.js&quot;)}
{!REQUIRESCRIPT(&quot;/soap/ajax/14.0/apex.js&quot;)}
if(&apos;{!QIRS_Action_Plan__c.Status__c}&apos; == &apos;Closed&apos;)
{
alert(&quot;Action Plan record status shouldn&apos;t be Closed&quot;); 
}
else
{
if(confirm(&apos;Are you sure you want to cancel Action Plan# {!QIRS_Action_Plan__c.Name}?&apos;)==true)
{ 
var result = sforce.apex.execute(&quot;QIRS_Release_Product_Controller&quot;, &quot;CanceledAP1&quot;,{AP_id:&quot;{!QIRS_Action_Plan__c.Id}&quot;});
if (result == &apos;false&apos;)
{
alert(&quot;Insufficient privilege, Contact to your Administrator&quot;);
}
else
if(&apos;{!QIRS_Action_Plan__c.Flag_for_Cancel__c}&apos; == false)
{
var full_url = document.URL; // Get current url
var url_array = full_url.split(&apos;/&apos;) // Split the string into an array with / as separator
var last_segment = url_array[url_array.length-1];  // Get the last part of the array (-1)
window.location = &apos;/apex/QIRS_Action_Plan_Cancel_Comments?id=&apos;+last_segment;
}
else
{
alert(&quot;Action Plan record once Canceled cannot be canceled again&quot;); 
}
}
}</url>
    </webLinks>
