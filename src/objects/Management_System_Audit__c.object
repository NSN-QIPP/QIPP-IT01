    <validationRules>
        <fullName>Audit_status_to_be_changed_to_complete</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Audit_Status__c , &apos;Completed&apos;) &amp;&amp; Audit_Complete_Date__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>Audit_Status__c</errorDisplayField>
        <errorMessage>Audit Status could not be changed to COMPLETED if the Audit Completed Date entered is in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Audit_status_to_be_changed_to_execute</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL( Audit_Status__c , &apos;Executed&apos;) &amp;&amp;  Audit_Executed_Date__c &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>Audit_Status__c</errorDisplayField>
        <errorMessage>Audit Status could not be changed to EXECUTED if the Audit Executed Date entered is in the future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>External_Audits_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>ISPICKVAL(Internal_External__c, &apos;External&apos;) &amp;&amp;  ($UserRole.Name != &apos;QIPP Audit Manager&apos;)</errorConditionFormula>
        <errorMessage>External Audits can only be created or modified by QMP.  Please contact QMP in this regard.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Audit_Completed_Date</fullName>
        <active>false</active>
        <description>Audit Complete Date can ONLY be entered when Audit Status = &quot;Completed“</description>
        <errorConditionFormula>AND (

NOT ( ISPICKVAL(  Audit_Status__c  , &apos;Completed&apos;) 
     ), 

 NOT(ISBLANK( Audit_Complete_Date__c )) 
)</errorConditionFormula>
        <errorDisplayField>Audit_Complete_Date__c</errorDisplayField>
        <errorMessage>Audit Complete Date can ONLY be entered when Audit Status = &quot;Completed“</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Audit_Executed_Date</fullName>
        <active>true</active>
        <description>Audit Executed Date can ONLY be entered when Audit Status = &quot;Executed&quot;, &quot;Reported&quot; and &quot;Completed&quot;</description>
        <errorConditionFormula>AND (

NOT ( 
 OR ( ISPICKVAL(  Audit_Status__c  , &apos;Executed&apos;), 
     ISPICKVAL(  Audit_Status__c  , &apos;Reported&apos;),
     ISPICKVAL(  Audit_Status__c  , &apos;Completed&apos;)
   )), 

 NOT(ISBLANK(Audit_Executed_Date__c)) 
)</errorConditionFormula>
        <errorDisplayField>Audit_Executed_Date__c</errorDisplayField>
        <errorMessage>Audit Executed Date can ONLY be entered when Audit Status = &quot;Executed&quot;, &quot;Reported&quot; or &quot;Completed&quot;</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Audit_Status</fullName>
        <active>true</active>
        <description>Audit Status cannot be set to &quot;Executed&quot;, &quot;Reported&quot; or &quot;Completed&quot; - when Audit Executed Date is NOT present</description>
        <errorConditionFormula>AND ( 

( 
OR ( ISPICKVAL( Audit_Status__c , &apos;Executed&apos;), 
ISPICKVAL( Audit_Status__c , &apos;Reported&apos;), 
ISPICKVAL( Audit_Status__c , &apos;Completed&apos;) 
)), 

(ISBLANK(Audit_Executed_Date__c)) 
)</errorConditionFormula>
        <errorDisplayField>Audit_Status__c</errorDisplayField>
        <errorMessage>Audit Status cannot be set to &quot;Executed&quot;, &quot;Reported&quot; or &quot;Completed&quot; - when Audit Executed Date is NOT present</errorMessage>
    </validationRules>
</CustomObject>
