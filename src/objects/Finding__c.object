        <encrypted>false</encrypted>
        <externalId>false</externalId>
        <formula>IF ( OR( ISPICKVAL( Finding_State__c , &apos;Complete&apos;), ISPICKVAL( Finding_State__c , &apos;Canceled&apos;)), TEXT(Finding_State__c), 
IF( Finding_Completion_Date_Forecast__c = Finding_Completion_Date_Target__c , &quot;On Plan&quot;, 

IF( Finding_Completion_Date_Forecast__c &gt; Finding_Completion_Date_Target__c , &quot;Behind&quot;, 

IF( Finding_Completion_Date_Forecast__c &lt; Finding_Completion_Date_Target__c , &quot;Ahead&quot;, 

IF( OR(ISBLANK( Finding_Completion_Date_Forecast__c ), ISBLANK( Finding_Completion_Date_Target__c ) ) , &quot;TBD&quot;, &quot;&quot; 

) 
) 
) 
) 
)</formula>
