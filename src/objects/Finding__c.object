    <validationRules>
        <fullName>Actual_Date_Future_Should_Be_Prevented</fullName>
        <active>true</active>
        <description>Future date entry in the “Actual” field should be restricted. It can have dates only either in past or present</description>
        <errorConditionFormula>Finding_Completion_Date_Actual__c  &gt; TODAY()</errorConditionFormula>
        <errorDisplayField>Finding_Completion_Date_Actual__c</errorDisplayField>
        <errorMessage>Actual Finding Completion Date Should Be Either In Past Or Present</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Target_Completion_Date_Past_Entry_Restrn</fullName>
        <active>true</active>
        <description>Past date entry in the “Target completion date ” field shall be restricted. It can have dates only either in present  or future, Rule would awake when Finding Start /Audit Execute Date left empty and restrn shall still be there refraining invalid entry.</description>
        <errorConditionFormula>AND(ISBLANK(FindingStartDate__c),Finding_Completion_Date_Target__c  &lt;  TODAY())</errorConditionFormula>
        <errorDisplayField>Finding_Completion_Date_Target__c</errorDisplayField>
        <errorMessage>Target completion date should be either in Present or Future</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Actual_Completion_Date</fullName>
        <active>false</active>
        <description>Cannot change Finding State to completed unless an ACTUAL date is entered for this Finding.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Finding_State__c, &quot;Complete&quot;), 

ISBLANK(Finding_Completion_Date_Actual__c ) 

)</errorConditionFormula>
        <errorDisplayField>Finding_Completion_Date_Actual__c</errorDisplayField>
        <errorMessage>Finding State cannot be set to &quot;Complete&quot; unless Actual completion date is populated.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Actual_Completion_Date_Entry</fullName>
        <active>true</active>
        <description>Actual Finding Completion Date is entered while Finding State value found other than “Complete”.</description>
        <errorConditionFormula>AND(NOT(ISBLANK( Finding_Completion_Date_Actual__c )) , NOT( ISPICKVAL( Finding_State__c , &apos;Complete&apos;) ))</errorConditionFormula>
        <errorDisplayField>Finding_Completion_Date_Actual__c</errorDisplayField>
        <errorMessage>Actual Finding Completion Date is entered while Finding State value found other than “Complete”.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Audit_Status</fullName>
        <active>true</active>
        <description>Findings can only be created when Audit Status equals &quot;In Progress&quot;, &quot;Reported&quot; or &quot;Executed&quot;.</description>
        <errorConditionFormula>AND( ISNEW(), 

NOT (OR( ISPICKVAL(Audit_Name__r.Audit_Status__c, &apos;In Progress&apos;), 
ISPICKVAL(Audit_Name__r.Audit_Status__c, &apos;Reported&apos;), ISPICKVAL(Audit_Name__r.Audit_Status__c, &apos;Executed&apos;)))
)</errorConditionFormula>
        <errorMessage>Findings can only be created when Audit Status equals &quot;In Progress&quot;, &quot;Reported&quot; or &quot;Executed&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validate_Record_Status</fullName>
        <active>false</active>
        <description>Cannot change Finding State to completed unless Record Status is set to &quot;Closed&quot; for this Finding.</description>
        <errorConditionFormula>AND( 
ISPICKVAL(Finding_State__c, &quot;Complete&quot;), 
NOT(ISPICKVAL(Record_Status__c, &quot;Closed&quot;))

)</errorConditionFormula>
        <errorDisplayField>Record_Status__c</errorDisplayField>
        <errorMessage>Finding State cannot be set to &quot;Complete&quot; unless  Record Status is set to &quot;Closed&quot;.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Validation_for_Target_Completion_Date</fullName>
        <active>true</active>
        <description>Target Completion Date should be greater than Finding Start Date / Audit Execute Date</description>
        <errorConditionFormula>Finding_Completion_Date_Target__c &lt;= FindingStartDate__c</errorConditionFormula>
        <errorDisplayField>Finding_Completion_Date_Target__c</errorDisplayField>
