    <apex:pageBlock title="Product Identification"> 
        <apex:pageBlockTable value="{!ProductIdentification}" var="Product">
            <apex:column value="{!Product.Name}"/> 
            <apex:column value="{!Product.Product_Name__c}"/>
            <apex:column value="{!Product.Technology__c}"/> 
            <apex:column headervalue="CreatedBy">                        
            <apex:outputText value="{!Product.CreatedBy.Name}"/>   
            </apex:column>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
            <apex:column headervalue="CreatedDate">                        
            <apex:outputText value="{!Product.CreatedDate}"/>   
            </apex:column>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;              
        </apex:pageBlockTable> 
    </apex:pageBlock>
    <apex:pageBlock title="Action Plans"> 
