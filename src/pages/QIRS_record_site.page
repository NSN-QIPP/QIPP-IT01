            <apex:column value="{!item.Name}"/> 
            <apex:column value="{!item.Action_Type__c}"/>
            <apex:column value="{!item.Champion__c}"/> 
            <apex:column value="{!item.Required_date__c}"/>
            <apex:column value="{!item.Approver__c}"/> 
            <apex:column value="{!item.Status__c}"/>
        </apex:pageBlockTable> 
    </apex:pageBlock>
    <apex:pageBlock title="Cost"> 
