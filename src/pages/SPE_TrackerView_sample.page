<apex:page standardController="SPE_SPETracker__c" extensions="SPE_TrackerView" contentType="application/vnd.ms-excel#SalesForceExport.xls" cache="true">    
   <style>    
    .button1 {
      display: inline-block;
      border-radius: 5px;
      background-color: #1797c0;
      background : #1797c0;
       
      border: none;
      color: #FFFFFF;
      text-align: center;
      font-size: 12px;
      padding: 14px;
      width: 140px;
      transition: all 0.5s;
      cursor: pointer;
      margin-top: -11px;
      margin-left: 50px;
    }
    
    .button1 span {
      cursor: pointer;
      display: inline-block;
      position: relative;
      transition: 0.5s;
    }

    .button1 span:after {
      content: 'Â»';
      position: absolute;
      opacity: 0;
      top: 0;
      right: -20px;
      transition: 0.5s;
    }

    .button1:hover span {
      padding-right: 25px;
    }
    
    .button1:hover span:after {
      opacity: 1;
      right: 0;
    }
    .tarckerHeader
     {
        border:1px solid black;
        background-color:#1797c0 ;
        color:White;
        text-align:left;
        padding:5px;
        height:70px;
        font-family: "Times New Roman";
     }
    .tableStyle
     {
        
        table-layout: fixed;
        margin-left: 20px;
        margin-top: 20px;
        border: 1.7px solid black;
        border-collapse: collapse;
        cellpadding="10"
        
    }
    .backgroungColor
     {
        background-color:#1797c0;
     }
     .fontColor
     {
        font-family: "Times New Roman";
        color:#FFFFFF;
        font-weight: 600;
     }
     tableBorder
     {
     border:1px solid black;
     border-collapse: collapse;
     }
     table.widget1 td 
     {
        border:1px solid black;
        
        
     }
     table.widget2 td
     {
        color:inherit;
