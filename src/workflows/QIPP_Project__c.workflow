    <rules>
        <fullName>Overdue Project Status Canceled after 180 days manual</fullName>
        <actions>
            <name>QIPP_System_workflow_change_project_state_to_Canceled_after_180_days_manual</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>QIPP_Reason_for_Cancelled_Manual</name>
            <type>FieldUpdate</type>
        </actions>
        <active>false</active>
        <criteriaItems>
            <field>QIPP_Project__c.Project_State__c</field>
            <operation>equals</operation>
            <value>Canceled</value>
        </criteriaItems>
        <description>Overdue Project Status Canceled after 180 days manual</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Overdue Project Status Cancelled after 180 days</fullName>
        <active>true</active>
        <description>Overdue Project Status Cancelled after 180 days</description>
        <formula>AND( 	 	ISPICKVAL(Project_State__c ,&apos;On Hold&apos;),  	NOT(ISPICKVAL(Check_Lean_Approval_State__c ,&apos;During Lean&apos;)), 	NOT(ISPICKVAL(Check_Approval_Process_Status__c ,&apos;During CIF&apos;)), 	NOT(ISPICKVAL(Check_Approval_Process_Status__c ,&apos;During Phase&apos;)), 	NOT(ISPICKVAL(Check_Approval_Process_Status__c ,&apos;During L6S&apos;)), 	NOT(ISPICKVAL(Check_DI_Approval_State__c ,&apos;During DI3&apos;)), 	NOT(ISPICKVAL(Check_DI_Approval_State__c ,&apos;During DI4&apos;)), 	NOT(ISPICKVAL(Check_DI_Approval_State__c ,&apos;During DI5&apos;)), 	NOT(ISPICKVAL(Check_DI_Approval_State__c ,&apos;During DI&apos;)), 	NOT(ISPICKVAL(FieldForControlVerifyPhase__c ,&apos;RecordLock&apos;)) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>QIPP_System_workflow_change_project_state_to_Canceled_after_180_days</name>
                <type>Alert</type>
            </actions>
            <actions>
                <name>QIPP_On_Hold_Project_to_Cancel</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>QIPP_Reason_for_Cancelled_Manual</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>QIPP_Uncheck_Re_Evaluation_Workflow</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
